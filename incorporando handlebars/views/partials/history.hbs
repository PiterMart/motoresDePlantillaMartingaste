<h3 class="text-primary">Productos</h3>
<br>
{{#if pLength }}
  <div class="table-responsive">
    <table class="table table-dark">
      <tr class="text-warning">
        <th class="font-weight-bold">Nombre</th>
        <th class="font-weight-bold">Precio</th>
        <th class="img">Foto</th>
      </tr>
      {{#each products }}
        <tr>
          <td>{{ this.nombre }}</td>
          <td>{{ this.precio }}</td>
          <td>{{ this.foto }}</td>
        </tr>
      {{/each}}
    </table>
  </div>
{{else}}
  <h3 class="alert alert-danger">No hay productos</h3>
{{/if}}
<p class="text"><a href="/"> Volver</a></p>